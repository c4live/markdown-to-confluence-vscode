{
  "name": "md2confl-vscode",
  "displayName": "Markdown to confluence for VSCode",
  "version": "0.0.1",
  "description": "VScode extension for convert markdown to confluence(atlassian wiki markup)",
  "main": "./out/extension",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint . --ext .ts,.tsx",
    "watch": "tsc -watch -p ./",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@types/node": "^14.0.23",
    "@types/vscode": "^1.47.0",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "eslint": "^7.4.0",
    "typescript": "^3.9.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/transnano/markdown-to-confluence-sample.git"
  },
  "keywords": [
    "markdown",
    "confluence",
    "convert"
  ],
  "engines": {
    "vscode": "^1.47.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.md2confl.convert"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.md2confl.convert",
        "title": "Convert markdown to confluence(atlassian wiki markup)"
      }
    ]
  },
  "author": "Transnano <transnano.jp@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/transnano/markdown-to-confluence-sample/issues"
  },
  "homepage": "https://github.com/transnano/markdown-to-confluence-sample#readme",
  "dependencies": {
    "@kenchan0130/markdown-to-atlassian-wiki-markup": "^3.0.6"
  }
}
